{
	"name": "@gmtdi/frontend-shared-components",
	"version": "1.0.1-56",
	"license": "MIT",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"files": [
		"dist"
	],
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"repository": {
		"url": "git://github.com/gmtdi/frontend-shared-components.git"
	},
	"scripts": {
		"predev": "yarn prebuild",
		"dev": "vite --port 3005",
		"prebuild": "node --trace-warnings component-list.js",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"clean": "rimraf dist",
		"build": "yarn clean && rollup -c --bundleConfigAsCjs",
		"release": "standard-version",
		"test": "jest --coverage --colors --silent --verbose --watchAll=false",
		"test:debug": "jest --watchAll --coverage --color --detectOpenHandles --silent false --verbose true --debug",
		"test:focus": "jest -i ./__tests__/helpers/FunctionHelpers.test.ts -t --watchAll --coverage --color --detectOpenHandles --silent false --verbose true --debug",
		"coverage": "open -a \"Google Chrome\" ./coverage/index.html",
		"test:clear": "jest --clearCache"
	},
	"dependencies": {
		"styled-components": "^6.1.1"
	},
	"devDependencies": {
		"@inrupt/jest-jsdom-polyfills": "^3.2.1",
		"@reduxjs/toolkit": "^2.2.4",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-sucrase": "^5.0.2",
		"@rollup/plugin-typescript": "^11.1.6",
		"@testing-library/jest-dom": "^6.4.5",
		"@testing-library/react": "^15.0.7",
		"@testing-library/user-event": "^14.5.2",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.12.11",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.0",
		"@types/react-redux": "^7.1.33",
		"@types/react-syntax-highlighter": "^15.5.13",
		"@types/styled-components": "^5.1.34",
		"@typescript-eslint/eslint-plugin": "^6.10.0",
		"@typescript-eslint/parser": "^6.10.0",
		"@vitejs/plugin-react": "^4.2.1",
		"@vitejs/plugin-react-swc": "^3.6.0",
		"directory-tree": "^3.5.1",
		"eslint": "8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-react": "^7.34.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-react-refresh": "^0.4.4",
		"html-react-parser": "^5.1.10",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-environment-node": "^29.7.0",
		"prettier": "^3.2.5",
		"prettier-plugin-organize-imports": "^3.2.4",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-element-to-jsx-string": "^15.0.0",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.23.1",
		"react-syntax-highlighter": "^15.5.0",
		"redux-persist": "^6.0.0",
		"rimraf": "^5.0.7",
		"rollup": "^4.17.2",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-dts": "^6.1.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-tsconfig-paths": "^1.5.2",
		"standard-version": "^9.5.0",
		"ts-jest": "^29.1.2",
		"typescript": "5.3.3",
		"vite": "^5.2.11",
		"vite-tsconfig-paths": "^4.3.2",
		"web-vitals": "^3.5.2"
	},
	"release": {
		"branches": [
			"main"
		]
	},
	"jest": {
		"preset": "ts-jest/presets/default-esm",
		"testEnvironment": "jest-environment-node",
		"coverageReporters": [
			"json",
			"html"
		],
		"coverageProvider": "v8",
		"transform": {
			"node_modules/variables/.+\\.(j|t)sx?$": "ts-jest",
			"^.+\\.(ts|tsx|js|jsx)?$": [
				"ts-jest",
				{
					"useESM": true,
					"tsconfig": {
						"jsx": "react-jsx"
					}
				}
			]
		},
		"moduleFileExtensions": [
			"js",
			"mjs",
			"cjs",
			"esm",
			"jsx",
			"ts",
			"tsx"
		],
		"moduleNameMapper": {
			"\\.(css|less|sass|scss)$": "<rootDir>/__mocks__/styleMock.ts",
			"component-list.json": "<rootDir>/src/component-list.json",
			"^components/(.*)": "<rootDir>/src/components/$1",
			"^datatypes/(.*)": "<rootDir>/src/datatypes/$1",
			"^helpers/(.*)": "<rootDir>/src/helpers/$1",
			"^hooks/(.*)": "<rootDir>/src/hooks/$1",
			"^modules/(.*)": "<rootDir>/src/modules/$1",
			"^store/(.*)": "<rootDir>/src/store/$1",
			"^styling/(.*)": "<rootDir>/src/styling/$1"
		},
		"transformIgnorePatterns": [
			"node_modules/(?!variables/.*)"
		]
	}
}
